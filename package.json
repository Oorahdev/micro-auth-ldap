{
  "name": "micro-auth-ldap",
  "description": "ldap auth microservice",
  "version": "1.0.6",
  "license": "MIT",
  "author": {
    "name": "Geir GÃ¥sodden",
    "email": "geir.gasodden@pythonia.no",
    "url": "https://github.com/zrrrzzt"
  },
  "main": "index.js",
  "engines": {
    "node": ">=7.5.0"
  },
  "scripts": {
    "test": "standard && nsp check && tap --reporter=spec test/**/*.js",
    "test-offline": "standard && tap --reporter=spec test/**/*.js",
    "coverage": "tap test/**/*.js --coverage",
    "coveralls": "tap --cov --coverage-report=lcov test/**/*.js && cat coverage/lcov.info | coveralls",
    "setup": "npm install",
    "standard-fix": "standard --fix",
    "start": "micro -p 8080",
    "dev": "micro",
    "now-deploy": "npm test && now --npm && now alias"
  },
  "keywords": [
    "microservice",
    "micro",
    "echo"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/telemark/micro-auth-ldap.git"
  },
  "bugs": {
    "url": "https://github.com/telemark/micro-auth-ldap/issues"
  },
  "now": {
    "alias": [
      "ldap.auth.mikrotjeneste.win"
    ],
    "env": {
      "NODE_ENV": "production"
    }
  },
  "devDependencies": {
    "coveralls": "2.11.16",
    "nsp": "2.6.2",
    "standard": "8.6.0",
    "tap": "10.2.0"
  },
  "dependencies": {
    "axios": "0.15.3",
    "jsonwebtoken": "7.3.0",
    "ldap-get-user": "1.0.9",
    "ldapauth-fork": "3.0.0",
    "marked": "0.3.6",
    "micro": "7.0.6",
    "simple-encryptor": "1.1.0",
    "urlencoded-body-parser": "2.0.0"
  }
}
